'.source.js':
  'Console log':
    prefix: 'cl'
    body: 'console.log($1);'

  'Console error':
    prefix: 'ce'
    body: 'console.error($1);'

  'Debugger':
    prefix: 'deb'
    body: 'debugger;'

  'For Max':
    prefix: 'formax'
    body: """
      for (var ${1:i} = 0, max = ${2:array}.length; ${1:i} < max; ${1:i}++) {
          ${3}
      }
    """
